				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-mtune=native" />
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DNOMFC" />
